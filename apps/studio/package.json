{
  "name": "@director.run/studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "bun run clean && BASE_PATH=/studio vite build && bun run storybook:build",
    "preview": "vite preview",
    "serve": "bun run server/index.ts",
    "clean": "rm -rf .turbo dist/*",
    "start": "bun run build && BASE_PATH=/studio bun run serve",
    "typecheck": "tsc --noEmit",
    "lint": "biome check ./src && biome check ./server",
    "format": "biome format --write ./src && biome format --write ./server",
    "storybook:dev": "storybook dev -p 6006",
    "storybook:build": "storybook build -o dist-storybook/",
    "spellcheck": "cspell \"**\""
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/react": "^9.1.5",
    "@storybook/react-vite": "^10.1.5",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^5.0.2",
    "cspell": "^9.2.2",
    "storybook": "^10.1.5",
    "typescript": "^5.8.3",
    "vite": "^7.2.7"
  },
  "dependencies": {
    "@director.run/client-configurator": "workspace:*",
    "@director.run/design": "workspace:*",
    "@director.run/gateway": "workspace:*",
    "@director.run/registry": "workspace:*",
    "@director.run/utilities": "workspace:*",
    "@phosphor-icons/react": "^2.1.10",
    "@tanstack/react-query": "^5.90.2",
    "@trpc/client": "^11.1.3",
    "@trpc/next": "^11.1.3",
    "@trpc/react-query": "^11.1.3",
    "@trpc/tanstack-react-query": "^11.1.3",
    "better-auth": "^1.4.6",
    "express": "^4.21.2",
    "next": "^16.0.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^7.0.0",
    "tailwindcss": "^4.1.17",
    "tailwindcss-radix": "^4.0.2",
    "tw-animate-css": "^1.4.0"
  }
}
